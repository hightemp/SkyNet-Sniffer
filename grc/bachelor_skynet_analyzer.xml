<?xml version="1.0"?>
<block>
  <name>SKYNET Receiverr</name>
  <key>bachelor_skynet_analyze</key>
  <category>bachelor</category>
  <import>import bachelor</import>
  <make>bachelor.skynet_analyze($samp,$symbols_per_second)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
    <param>
       <name>Sample Rate</name>
       <key>samp</key>
       <type>int</type>
     </param>

     <param>
       <name>Samples per Symbol</name>
       <key>symbols_per_second</key>
       <type>float</type>
     </param>
  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
       <sink>
         <name>trig</name>
         <type>float</type>
       </sink>

       <sink>
         <name>in</name>
         <type>float</type>
       </sink>

       <sink>
         <name>rssi</name>
         <type>float</type>
         <optional>1</optional>
       </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>message</type>
    <optional>1</optional>
  </source>
</block>
